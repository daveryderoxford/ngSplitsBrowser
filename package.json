{
  "name": "splitsbrowser",
  "version": "0.0.2-alpha2",
  "license": "MIT",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve": "ng serve",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' git.version.ts && ng build",
    "prod": "ts-node -O '{\"module\": \"commonjs\"}' git.version.ts && ng build --prod",
    "deploy": "npm run prod && firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-stats": "ng build --prod --statsJson && webpack-bundle-analyzer dist/stats.json",
    "sme": "source-map-explorer",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "servedist": "cd dist && python -m SimpleHTTPServer 8000"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.1",
    "@angular/cdk": "10.0.0",
    "@angular/common": "10.0.1",
    "@angular/compiler": "10.0.1",
    "@angular/core": "10.0.1",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "10.0.1",
    "@angular/material": "10.0.0",
    "@angular/platform-browser": "10.0.1",
    "@angular/platform-browser-dynamic": "10.0.1",
    "@angular/platform-server": "10.0.1",
    "@angular/pwa": "^0.901.0",
    "@angular/router": "10.0.1",
    "@angular/service-worker": "10.0.1",
    "@ngneat/until-destroy": "^7.1.5",
    "angular-split": "^3.0.3",
    "core-js": "^3.6.5",
    "d3": "^5.7.0",
    "d3-collection": "^1.0.7",
    "date-fns": "^2.14.0",
    "firebase": "^7.13.1",
    "firebase-admin": "^8.13.0",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.2.1",
    "leaflet": "^1.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.2",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "10.0.0",
    "@angular/compiler-cli": "10.0.1",
    "@firebase/testing": "^0.3.3",
    "@types/d3": "^5.0.1",
    "@types/jasmine": "^3.5.11",
    "@types/jasmine-expect": "^3.8.1",
    "@types/jquery": "^3.3.22",
    "@types/leaflet": "^1.4.3",
    "@types/lodash": "^4.14.146",
    "@types/node": "^14.0.14",
    "@types/stripe-checkout": "^1.0.3",
    "@types/stripe-v3": "^3.1.9",
    "codelyzer": "^5.2.2",
    "firebase-tools": "^6.10.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-expect": "^3.8.4",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
