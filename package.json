{
  "name": "splitsbrowser",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "serve": "ng serve",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' git.version.ts && ng build",
    "prod": "ts-node -O '{\"module\": \"commonjs\"}' git.version.ts && ng build --prod",
    "deploy": "npm run prod && firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-stats": "ng build --prod --statsJson && webpack-bundle-analyzer dist/stats-es2015.json",
    "servedist": "cd dist && python -m SimpleHTTPServer 8000"
  },
  "private": true,
