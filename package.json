{
  "name": "splitsbrowser",
  "version": "0.0.2-alpha2",
  "license": "MIT",
  "scripts": {
    "serve": "ng serve --configuration development",
    "dev": "ng build --configuration development --watch",
    "stats": "ng build --stats-json --named-chunks && npx @angular-experts/hawkeye dist/splitsbrowser/stats.json",
    "prod": "ng build --configuration production",
    "deploy": "npm run prod && firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "servedist": "cd dist/splitsbrowser/browser && python3 -m http.server 8000"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^21.0.5",
    "@angular/common": "21.0.6",
    "@angular/compiler": "21.0.6",
    "@angular/core": "21.0.6",
    "@angular/fire": "^20.0.0",
    "@angular/forms": "21.0.6",
    "@angular/material": "^21.0.5",
    "@angular/material-date-fns-adapter": "^21.0.5",
    "@angular/platform-browser": "21.0.6",
    "@angular/platform-browser-dynamic": "21.0.6",
    "@angular/platform-server": "21.0.6",
    "@angular/router": "21.0.6",
    "@ngbracket/ngx-layout": "15.1.3",
    "@ngneat/until-destroy": "^9.2.3",
    "chart.js": "^4.5.0",
    "core-js": "^3.6.5",
    "d3": "^7.9.0",
    "jquery": "^3.2.1",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.2100.4",
    "@angular/build": "^21.0.4",
    "@angular/cli": "21.0.4",
    "@angular/compiler-cli": "21.0.6",
    "@types/d3": "^7.4.3",
    "@types/jquery": "^3.3.22",
    "@types/node": "^22.10.2",
    "firebase-tools": "^14.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jsdom": "^24.0.0",
    "npm": "^6.14.4",
    "ts-node": "^10.9.1",
    "typescript": "5.9.3",
    "vite-plugin-static-copy": "^1.0.0",
    "vitest": "^4.0.8"
  },
  "overrides": {
    "@angular/fire": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core",
      "@angular/platform-browser": "$@angular/platform-browser",
      "@angular/platform-browser-dynamic": "$@angular/platform-browser-dynamic",
      "@angular/router": "$@angular/router",
      "@angular/platform-server": "$@angular/platform-server"
    }
  }
}